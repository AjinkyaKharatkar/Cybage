<message>
	<head>
		<messageid><!-- Unique Message id --></messageid>
		<date><!-- Use current date --></date>
		<messagetype>CatalogImport</messagetype>
		<sender>
			<systemid/>
			<companyid/>
			<replytoq/>
		</sender>
		<recipient>
			<systemid/>
			<companyid/>
			<replytoq/>
		</recipient>
	</head>
	<body>
		<Catalogimport>
			<statuscode />
			<itemtypename/>
			<uomname />
			<catalogitem><!-- Value of ClassLevel1Id--></catalogitem>
			<catalogname><!-- Value of ClassLevel1Name--></catalogname>
			<catalognotes/>
			<retailprice><!--Value of Price --></retailprice>
			<totalprice><!--Value of sum of All SKU QTY * Price --></totalprice>
			<shortdescription><!--Value of ProductDescription --></shortdescription>
			<totalorderquantity><!--Value of All SKU QTY --></totalorderquantity>
			<startdate><!-- Use current date --></startdate>
			<enddate/>
			<catalogcode><!-- Value of Style + Value of Gender --></catalogcode>
			<dutytax><!--Value of Tax --></dutytax>
			<skus>
				<!-- List of all sku avaialble in one product -->
					<sku>....</sku>
					<sku>....</sku>
					<sku>....</sku>.....
			</skus>			
		</Catalogimport>
	</body>
</message>
